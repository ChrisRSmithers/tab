  "scripts": {
    "start": "yarn run relay && nodemon --watch data/schema.graphql --exec yarn run start:react-app & yarn run relay:watch",
    "start:react-app": "cross-env NODE_PATH=src/ node scripts/start.js",
    "build": "yarn run relay && cross-env NODE_PATH=src/ node scripts/build.js",
    "postinstall": "patch-package",
    "deploy": "npm-run-all -s build sls:s3sync sls:deploy",
    "relay": "relay-compiler --src ./src/ --schema ./data/schema.graphql",
    "relay:watch": "nodemon --watch src/ --watch data/schema.graphql --ignore **/__generated__/**/* --exec \"yarn run relay\"",
    "test": "cross-env NODE_PATH=src/ npm-run-all -s test:lint test:run test:codecov",
    "test:run": "cross-env NODE_PATH=src/ jest --env=jsdom --testPathIgnorePatterns=e2e-tests --testPathIgnorePatterns=prebidPatches --maxWorkers=2 --coverage",
    "test:integration": "cross-env NODE_PATH=src/ jest --testPathPattern=e2e-tests",
    "test:codecov": "if-env LOG_TEST_COVERAGE=true && codecov || echo 'Skipping test coverage logging.'",
    "test:watch": "cross-env NODE_PATH=src/ jest --env=jsdom --testPathIgnorePatterns=e2e-tests --testPathIgnorePatterns=prebidPatches --watch",
    "build:langs": "node scripts/translate.js",
    "test:lint": "standard",
    "lint:fix": "standard --fix",
    "prebid:build": "npm-run-all -s prebid:install prebid:generate prebid:cp",
    "prebid:install": "cd ./node_modules/prebid.js/ && yarn install",
    "prebid:generate": "cd ./node_modules/prebid.js/ && ./node_modules/gulp/bin/gulp.js build  --modules \"../../src/js/ads/prebid/modules.json\"",
    "prebid:cp": "cp ./node_modules/prebid.js/build/dist/prebid.js ./src/js/ads/prebid/",
    "prebid:create-patches": "yarn patch-package prebid.js --include '^(src|modules)/'",
    "prebid:test-patches": "cross-env NODE_PATH=node_modules/prebid.js/ jest prebidPatches",
    "sls:deploy": "serverless deploy -v",
    "sls:s3sync": "node scripts/syncToS3.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/__generated__/**"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules[/\\\\](?!(prebid.js)[/\\\\])"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "ignore": [
      "src/js/ads/prebid/prebid.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gladly-team/tab.git"
  },
  "author": "Gladly Team",
  "bugs": {
    "url": "https://github.com/gladly-team/tab/issues"
  },
  "optionalDependencies": {
    "material-ui-codemod": "^1.0.0-beta.11"
  }
}
