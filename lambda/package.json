{
  "name": "tab-lambda",
  "version": "0.1.0",
  "description": "Tab for a Cause.",
  "private": true,
  "dependencies": {
    "asyncawait": "^1.0.6",
    "aws-sdk": "^2.52.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.0.0-beta.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "codecov": "^2.1.0",
    "cors": "^2.8.1",
    "cross-env": "^5.0.0",
    "dotenv-extended": "^2.0.0",
    "express": "^4.14.1",
    "if-env": "^1.0.0",
    "jest": "^20.0.1",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "serverless": "^1.14.0",
    "serverless-plugin-include-dependencies": "^2.1.2",
    "yamljs": "^0.2.8"
  },
  "scripts": {
    "start": "nodemon -e js,json,yml --exec \"babel-node\" ./server.js",
    "deploy": "serverless deploy",
    "build": "rm -rf ./build && npm run build:compile",
    "build:compile": "cross-env NODE_ENV=production babel --out-dir='build' --ignore='build,coverage,__mocks__,__tests__,node_modules' ./",
    "test": "npm-run-all -s test:run test:codecov",
    "test:run": "jest --coverage",
    "test:codecov": "if-env NODE_ENV=test && codecov || echo 'Skipping test coverage logging.'",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gladly-team/tab.git"
  },
  "author": "Gladly Team",
  "bugs": {
    "url": "https://github.com/gladly-team/tab/issues"
  },
  "homepage": "https://github.com/gladly-team/tab#readme"
}
